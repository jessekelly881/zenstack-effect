{
  "name": "zenstack-effect",
  "displayName": "Effect Schema Generator",
  "version": "0.0.2",
  "description": "Generate Effect Schemas from ZenStack",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jessekelly881/zenstack-effect"
  },
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": true
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run lint && npm run clean && tsc && copyfiles ./package.json ./README.md ./README-1.svg ./README-2.svg ./LICENSE ./static/* dist",
    "dev": "zenstack generate --output codegen --no-compile",
    "watch": "tsc --watch",
    "lint": "eslint src --ext ts",
    "test": "ts-node test.ts"
  },
  "author": "Jesse Kelly",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/anthropic": "^1.0.6",
    "@ai-sdk/openai": "^1.0.9",
    "@ai-sdk/xai": "^1.0.5",
    "@effect/platform": "^0.72.1",
    "@effect/platform-node": "^0.68.1",
    "@prisma/generator-helper": "^5.6.0",
    "@zenstackhq/runtime": "^2.10.0",
    "@zenstackhq/sdk": "^2.10.0",
    "ai": "^4.0.12",
    "dotenv": "^16.4.7",
    "effect": "^3.12.1",
    "typescript": "^5.7.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@prisma/client": "^5.6.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "copyfiles": "^2.4.1",
    "cpx": "^1.5.0",
    "eslint": "^8.35.0",
    "prisma": "^5.6.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "zenstack": "^2.10.0"
  }
}
